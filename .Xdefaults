*TkTheme: clearlooks

! Xft settings ---------------------------------------------------------------
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
!Xft.hinting:    full
Xft.hinting:    slight
Xft.autohint:   0
Xft.lcdfilter:  lcddefault

! urxvt ----------------------------------------------------------------------
!URxvt*foreground:			#FFFFFF
!URxvt*background:   		#1d1f21
!URxvt*cursorColor:  		#aeafad

!URxvt.font:             	 xft:Inconsolata for Powerline:pixelsize=15:antialias=false
!URxvt.font:             	 xft:Source Code Pro for Powerline:pixelsize=13:antialias=true:hinting=full
!URxvt.font:             	xft:Ubuntu Mono derivative Powerline:pixelsize=14
!URxvt.letterSpace:			-1

!URxvt.font: xft:Droid Sans Mono for Powerline:pixelsize=11:antialias=true
!URxvt.letterSpace:			-2

!URxvt*font: -*-envy code r-medium-r-*-*-12-*-*-*-*-*-iso10646-1
!URxvt*boldFont: -*-envy code r-medium-r-*-*-12-*-*-*-*-*-iso10646-1
!URxvt.letterSpace:			 0

!URxvt.font: -*-termsyn.icons-medium-r-*-*-14-*-*-*-*-*-*-*
URxvt.font: -*-termsyn-medium-r-*-*-14-*-*-*-*-*-*-*
!URxvt.boldFont: -*-termsyn-medium-r-*-*-14-*-*-*-*-*-*-*

URxvt.xftAntialias:     	false
Urxvt*transparent:			true
URxvt*shading:          	20
URxvt*saveLines:        	12000
URxvt*visualBell:       	true
URxvt*urgentOnBell:     	true
URxvt*scrollBar:        	false
URxvt*loginShell:       	true
URxvt*inheritPixmap:    	true
URxvt*print-pipe:       	cat > $(echo URxvt**.dump.$(date +'%Y%M%d%H%m%S'))
URxvt*perl-lib:         	/usr/lib/urxvt/perl/
URxvt.perl-ext-common:  	default,url-select,keyboard-select
URxvt*perl-ext:         	selection,clipboard
URxvt*url-select.launcher:	/usr/bin/chromium
URxvt*matcher.button:		1
URxvt*colorUL:          	2
URxvt*cutchars:         	`""()'*'<>[]{|}
URxvt*keysym.M-u:       	perl:url-select:select_next
URxvt*keysym.M-Escape:  	perl:keyboard-select:activate
URxvt*keysym.M-s:       	perl:keyboard-select:search
URxvt*underlineURLs:    	true
URxvt*url-select.underline: true
URxvt*termName:				rxvt-256color
URxvt*internalBorder:       4
URxvt*externalBorder:       0

*cursorColor:                         #917154
!*colorIT:                             #E3AB8D
!*colorBD:                             #F0E5DF
!*colorUL:                             #cd546c
!*VT100.colorBDMode:                   true
!*VT100.colorBD:                       red
!*VT100.colorULMode:                   true
!*VT100.colorUL:                       magenta
!*VT100.veryBoldColors:                6

URxvt*foreground:                     #eddcd3
URxvt*background:                     #181512

