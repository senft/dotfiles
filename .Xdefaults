*cursorColor:                         #eddcd3

URxvt*foreground:                     #eddcd3
URxvt*background:                     #121212

*TkTheme: clearlooks

! Xft settings ---------------------------------------------------------------
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
!Xft.hinting:    full
Xft.hinting:    slight
Xft.autohint:   0
Xft.lcdfilter:  lcddefault

! urxvt ----------------------------------------------------------------------
!URxvt*foreground:			#FFFFFF
!URxvt*background:   		#1d1f21
!URxvt*cursorColor:  		#aeafad

!URxvt*font: -*-envy code r-medium-r-*-*-10-*-*-*-*-*-iso10646-1
!URxvt*boldFont: -*-envy code r-medium-r-*-*-10-*-*-*-*-*-iso10646-1
!URxvt.letterSpace:			 0

!URxvt.font: -*-termsyn-medium-r-*-*-13-*-*-*-*-*-*-*
!URxvt.boldFont: -*-termsyn-medium-r-*-*-13-*-*-*-*-*-*-*

URxvt.font: -misc-tamzenforpowerline-medium-r-normal--12-87-100-100-c-60-iso10646-1
URxvt.boldFont: -misc-tamzenforpowerline-medium-r-normal--12-87-100-100-c-60-iso10646-1

URxvt.xftAntialias:     	false
!Urxvt*transparent:			true
!URxvt*shading:          	12
!URxvt*inheritPixmap:    	true
!URxvt*depth:                32
URxvt*saveLines:        	12000
URxvt*visualBell:       	true
URxvt*urgentOnBell:     	true
URxvt*scrollBar:        	false
URxvt*loginShell:       	true
URxvt*print-pipe:       	cat > $(echo URxvt**.dump.$(date +'%Y%M%d%H%m%S'))
URxvt*perl-lib:         	/usr/lib/urxvt/perl/
URxvt.perl-ext-common:  	default,url-select,keyboard-select
URxvt*perl-ext:         	selection,clipboard
URxvt*url-select.launcher:	firefox
URxvt*matcher.button:		1
URxvt*colorUL:          	2
URxvt*cutchars:         	`""()'*'<>[]{|}
URxvt*keysym.M-u:       	perl:url-select:select_next
URxvt*keysym.M-Escape:  	perl:keyboard-select:activate
URxvt*keysym.M-s:       	perl:keyboard-select:search
URxvt*underlineURLs:    	true
URxvt*url-select.underline: true
URxvt*termName:				rxvt-256color
URxvt*internalBorder:       4
URxvt*externalBorder:       0
