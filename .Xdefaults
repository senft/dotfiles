! Xft settings ---------------------------------------------------------------
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull
Xft.autohint:   0
Xft.lcdfilter:  lcddefault

! urxvt ----------------------------------------------------------------------
URxvt*iconFile: 		/usr/share/icons/Faenza/apps/96/lxterminal.png
URxvt.font:             -*-envy code r-medium-r-*-*-13-*-*-*-*-*-iso10646-*
URxvt.boldFont:         -*-envy code r-bold-r-*-*-13-*-*-*-*-*-iso10646-*
URxvt.xftAntialias:     false
!URxvt.font: 			-xos4-terminus-medium-r-normal--14-140-72-72-*-*-*-*
!URxvt.boldFont: 		-xos4-terminus-bold-r-normal--14-140-72-72-*-*-*-*
URxvt*shading:          20 
URxvt*saveLines:        12000
URxvt*visualBell:       true
URxvt*urgentOnBell:     true
URxvt*scrollBar:        false
URxvt*loginShell:       true
URxvt*inheritPixmap:    true
URxvt*print-pipe:       cat > $(echo URxvt**.dump.$(date +'%Y%M%d%H%m%S'))
URxvt*perl-lib:         /usr/lib/urxvt/perl/
URxvt.perl-ext-common:  default,url-select
URxvt*perl-ext:         selection,clipboard
URxvt*urlLauncher:      /usr/bin/chromium-browser
URxvt*matcher.button:   1
URxvt*colorUL:          2
URxvt*cutchars:         `""()'*'<>[]{|}
URxvt.keysym.M-u:       perl:url-select:select_next
URxvt.underlineURLs:    true

! tab colors
URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 9
URxvt.tabbed.tab-bg: 0
URxvt.tabbed.autohide: 1

! xscreensaver ---------------------------------------------------------------
!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.headingFont:        -*-envy code r-bold-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.bodyFont:           -*-envy code r-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.labelFont:          -*-envy code r-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.buttonFont:         -*-envy code r-bold-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.passwd.passwdFont:         -*-envy code r-bold-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.foreground:         #ffffff
xscreensaver.Dialog.background:         #151515
xscreensaver.Dialog.topShadowColor:     #000000
xscreensaver.Dialog.bottomShadowColor:  #000000
xscreensaver.Dialog.Button.foreground:  #FFFFFF
xscreensaver.Dialog.Button.background:  #303030
!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #000000
xscreensaver.Dialog.text.background:    #ffffff
xscreensaver.Dialog.internalBorderWidth:10
xscreensaver.Dialog.borderWidth:        1
xscreensaver.Dialog.shadowThickness:    1
!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  #303030
xscreensaver.passwd.thermometer.background:  #000000
xscreensaver.passwd.thermometer.width:       4
!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    %I:%M%P %a %b %d, %Y
