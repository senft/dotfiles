! terminal colors ------------------------------------------------------------
! tangoesque scheme
*background: #000000
*foreground: #FFFFFF
! Black (not tango) + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
!*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #93d44f
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204a87
! DarkMangenta + Mangenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan (both not tango)
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff

! Xft settings ---------------------------------------------------------------
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

! urxvt ----------------------------------------------------------------------
URxvt*background:       black
URxvt*foreground:       white
URxvt*font:             -*-terminus-medium-*-normal-*-14-*-*-*-*-*-*-*
URxvt*shading:	   	    20 
URxvt*saveLines:		12000
URxvt*visualBell:		true
URxvt*urgentOnBell:		true
URxvt*scrollBar:		false
URxvt*loginShell:		true
URxvt*inheritPixmap:    true
URxvt*print-pipe:		cat > $(echo URxvt**.dump.$(date +'%Y%M%d%H%m%S'))
URxvt*perl-lib:         /usr/lib/urxvt/perl/
URxvt*perl-ext-common:  default,matcher
URxvt*perl-ext: 		selection,mark-yank-urls,clipboard
URxvt*keysym.M-u: 		perl:mark-yank-urls:activate_mark_mode
URxvt*urlLauncher:      /usr/bin/chromium
URxvt*matcher.button:   1
URxvt*colorUL: 			2
URxvt*cutchars: 		`""()'*'<>[]{|}
