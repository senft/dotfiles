! Black + DarkGrey
*color0:       #000000
*color8:       #666666

! DarkRed + Red
*color1:       #912226
*color9:       #cc6666

! DarkGreen + Green
*color2:       #778900
*color10:      #b5bd68

! DarkYellow + Yellow
*color3:       #ae7b00
*color11:      #f0c674

! DarkBlue + Blue
*color4:       #1d2594
*color12:      #81a2be

! DarkMangenta + Mangenta
*color5:       #682a9b
*color13:      #b294bb

!DarkCyan + Cyan
*color6:       #2b6651
*color14:      #8abeb7

! LightGrey + White
*color7:       #929593
*color15:      #ecebec

*TkTheme: clearlooks

! Xft settings ---------------------------------------------------------------
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
!Xft.hinting:    full
Xft.hinting:    slight
Xft.autohint:   0
Xft.lcdfilter:  lcddefault

! urxvt ----------------------------------------------------------------------
URxvt*foreground:			#FFFFFF
URxvt*background:   		#1d1f21
URxvt*cursorColor:  		#aeafad
URxvt*iconFile:				/usr/share/icons/Faenza/apps/96/lxterminal.png
!URxvt.font:            	 -*-envy code r-medium-r-*-*-13-*-*-*-*-*-iso10646-*
!URxvt.boldFont:        	 -*-envy code r-bold-r-*-*-13-*-*-*-*-*-iso10646-*
!URxvt.letterSpace:			0
URxvt.font:             	xft:Envy Code R for Powerline:pixelsize=13:antialias=false
URxvt.boldFont:         	xft:Envy Code R for Powerline:bold:pixelsize=13:antialias=false
URxvt.letterSpace:			-2

URxvt.xftAntialias:     	false
urxvt*transparent:			true
URxvt*shading:          	30 
URxvt*saveLines:        	12000
URxvt*visualBell:       	true
URxvt*urgentOnBell:     	true
URxvt*scrollBar:        	false
URxvt*loginShell:       	true
URxvt*inheritPixmap:    	true
URxvt*print-pipe:       	cat > $(echo URxvt**.dump.$(date +'%Y%M%d%H%m%S'))
URxvt*perl-lib:         	/usr/lib/urxvt/perl/
URxvt.perl-ext-common:  	default,url-select,keyboard-select
URxvt*perl-ext:         	selection,clipboard
URxvt*url-select.launcher:	/usr/bin/chromium
URxvt*matcher.button:		1
URxvt*colorUL:          	2
URxvt*cutchars:         	`""()'*'<>[]{|}
URxvt.keysym.M-u:       	perl:url-select:select_next
URxvt.keysym.M-Escape:  	perl:keyboard-select:activate
URxvt.keysym.M-s:       	perl:keyboard-select:search
URxvt.underlineURLs:    	true
URxvt.url-select.underline: true
URxvt*internalBorder:		0
URxvt*termName:				rxvt-256color
